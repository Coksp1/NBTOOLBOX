
endogenous
y   // Output gap
pie // Inflation gap
i   // Interest rate gap
r;  // Real interest rate gap

exogenous
eps // Demand shock
eta // Supply shock
e;  // Monetary policy shock

parameters
alpha
beta
gamma_eps
gamma_eta
gamma_i
gamma_pie
gamma_y
phi
theta
varphi;

model

    // IS relation
    y   = theta*y(-1) + (1-theta)*y(+1) - varphi*(i - pie(+1)) + eps;

    // Phillips curve
    pie = phi*pie(-1) + (1-phi)*beta*pie(+1) + alpha*y + eta;

    // Interest rate rule
    i   = gamma_i*i(-1) + (1-gamma_i)*(gamma_pie*pie(-1) + gamma_y*y(-1) + gamma_eps*eps + gamma_eta*eta) + e; 

    // Real rate
    r   = i - pie(+1);

end

varobs
y
pie
i
;





